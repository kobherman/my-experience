<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">MNewton:=proc (f, a, b, eps, maxit)
local f_pr, f_pr2, x_0, x_t, err, m, iters, i;
</Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">if f(a)*f(b) &gt;= 0 then
    err:=1;
    return 0, -1, err;
else
</Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">    err:=0;
</Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">    f_pr:=diff(f(x), x);</Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">    f_pr2:=diff(f_pr, x);</Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">    if (f(a)*subs(x=a,f_pr2) &gt; 0) then
        x_0 := a;
    elif (f(b)*subs(x=b,f_pr2) &gt; 0) then
        x_0 := b;
    else
        err:=2;
    end if;
</Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">    if err&lt;&gt;2 then
        m:=minimize(abs(f_pr), x=a..b);
        iters:=0;
        x_t:= x_0;</Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">        for i from 1 to maxit while (abs(f(x_t)) &gt;= epsilon*m) do
            x_t:=evalf(x_t - f(x_t)/subs(x=x_t,f_pr));
            iters:= iters+1;
        end do;
    </Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">        if i=maxit+1 then err:=3; end if;
        return  x_t, iters, err;
    
    </Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">    else
        return 0, -1, err;

    end if;
end if;
end proc:</Text-field>
