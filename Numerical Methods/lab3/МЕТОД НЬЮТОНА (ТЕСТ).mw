        x_n1:= subs(x=x_n, phi);
        iters:= iters + 1;
    end do;
    </Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">    if i=maxit+1 then
        err:=3;
    end if;
    
    return  x_n1, iters, err;
        </Text-field>
